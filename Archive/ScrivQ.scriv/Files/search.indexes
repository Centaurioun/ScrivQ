            &lt;text variable="note" suffix=". "/&gt;
            &lt;text variable="original-title" display="indent" suffix=" "/&gt;
            &lt;text variable="original-publisher-place" form="short"/&gt;
            &lt;text variable="call-number" font-weight="normal" display="indent" suffix=" "/&gt;
