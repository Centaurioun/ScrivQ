        &lt;/group&gt;
      &lt;/if&gt;
      &lt;else-if type="patent"&gt;
        &lt;group delimiter=" " prefix=". "&gt;
          &lt;text variable="authority"/&gt;
          &lt;text variable="number"/&gt;
        &lt;/group&gt;
      &lt;/else-if&gt;
